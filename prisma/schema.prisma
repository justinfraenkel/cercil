datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}
generator client {
  provider = "prisma-client-js"
}
model Member {
  id      String   @id @default(uuid())
  name    String
  code    String   @unique
  created DateTime @default(now())
}
model InviteToken {
  id          String   @id @default(uuid())
  code        String   @unique
  issuerId    String   @map("issuer_id")
  issuedAt    DateTime @default(now())
  redeemedBy  String?
  redeemedAt  DateTime?
  @@index([code])
}
